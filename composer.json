{
    "name": "ocramius/ocra-cached-view-resolver",
    "description": "Cached view template path resolver to speed up the ZF2 view layer by avoiding stat calls",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "zf2",
        "zendframework",
        "zend framework 2",
        "performance"
    ],
    "homepage": "https://github.com/Ocramius/OcraCachedViewResolver",
    "authors": [
        {
            "name": "Marco Pivetta",
            "email": "ocramius@gmail.com",
            "homepage": "http://ocramius.github.com/"
        }
    ],
    "require": {
        "php":                            "^7.4.7 || ~8.0.0",
        "laminas/laminas-cache":          "^3.1.2",
        "laminas/laminas-modulemanager":  "^2.10.2",
        "laminas/laminas-mvc":            "^3.3.0",
        "laminas/laminas-servicemanager": "^3.10.0",
        "laminas/laminas-view":           "^2.19.0"
    },
    "require-dev": {
        "doctrine/coding-standard":                     "^9.0.0",
        "laminas/laminas-cache-storage-adapter-memory": "^2.0.0",
        "phpunit/phpunit":                              "^9.5.11",
        "vimeo/psalm":                                  "^4.18"
    },
    "autoload": {
        "psr-0": {
            "OcraCachedViewResolver\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-0": {
            "OcraCachedViewResolverTest\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.1.x-dev"
        }
    }
}
