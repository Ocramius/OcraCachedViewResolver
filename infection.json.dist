{
    "source": {
        "directories": [
            "src"
        ]
    },
    "logs": {
        "text": "php://stderr",
        "github": true,
        "stryker": {
            "report": "/^\\d+\\.\\d+\\.x$/"
        }
    },
    "mutators": {
        "@default": true
    },
    "minMsi": 73,
    "minCoveredMsi": 80
}
